                            select#multiselect6(multiple='multiple')
                              option(value='cheese') Cheese
                              option(value='tomatoes') Tomatoes
                              option(value='mozarella') Mozzarella
                              option(value='mushrooms') Mushrooms
                              option(value='pepperoni') Pepperoni
                              option(value='onions') Onions
                        .form-group
                          .col-md-12.caret-abs
                            select#multiselect7.multiselect-withlabels(multiple='multiple')
                              optgroup(label='Mathematics')
                                option(value='analysis') Analysis
                                option(value='discrete') Discrete Mathematics
                                option(value='probability') Probability Theory
                              optgroup(label='Computer Science')
                                option(value='programming') Introduction to Programming
                                option(value='automata') Automata Theory
                                option(value='complexity') Complexity Theory
                        .form-group
                          .col-md-12.caret-abs
                            select#multiselect8(multiple='multiple')
                              option(value='cheese') Cheese
                              option(value='tomatoes') Tomatoes
                              option(value='mozarella') Mozzarella
                              option(value='mushrooms') Mushrooms
                              option(value='pepperoni') Pepperoni
                              option(value='onions') Onions
              // Plugin: Dual Select List
              .panel
                form#demoform.hide-list-label(action='#', method='post')
