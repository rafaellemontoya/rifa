                      .col-lg-9
                        .input-group
                          span.input-group-addon
                            i.fa.fa-usd
                          input#maskedMoney.form-control.money(type='text', maxlength='10', autocomplete='off', placeholder='000.000.000.000')
                    .form-group
                      label.col-lg-2.control-label(for='maskedKey') Product Key
                      .col-lg-9
                        .input-group
                          span.input-group-addon
                            i.fa.fa-key
                          input#maskedKey.form-control.product(type='text', maxlength='10', autocomplete='off', placeholder='000.000.000.000,00')
                    .form-group
                      label.col-lg-2.control-label(for='maskedID') Tax ID
                      .col-lg-9
                        .input-group
                          span.input-group-addon
                            i.fa.fa-gavel
                          input#maskedID.form-control.tin(type='text', maxlength='10', autocomplete='off', placeholder='99999-999')
                    .form-group
                      label.col-lg-2.control-label(for='maskedSSN') SSN
