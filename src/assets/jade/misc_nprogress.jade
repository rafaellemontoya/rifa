            .col-md-6
              ul.list-group.controls.controls2
                li.list-group-item.bg-light
                  h4.mv5 Page Loader Contextuals
                li.list-group-item.list-group-item-primary
                  button#c-primary.btn.btn-default.btn-sm.play.mr10
                    i.fa.fa-play
                  |  Contextual
                  b.text-primary  - Primary
                li.list-group-item.list-group-item-success
                  button#c-success.btn.btn-default.btn-sm.play.mr10
                    i.fa.fa-play
                  |  Contextual
                  b.text-success  - Success
                li.list-group-item.list-group-item-info
                  button#c-info.btn.btn-default.btn-sm.play.mr10
                    i.fa.fa-play
                  |  Contextual
                  b.text-info  - Info
                li.list-group-item.list-group-item-warning
                  button#c-warning.btn.btn-default.btn-sm.play.mr10
                    i.fa.fa-play
                  |  Contextual
                  b.text-warning  - Warning
                li.list-group-item.list-group-item-danger
                  button#c-danger.btn.btn-default.btn-sm.play.mr10
                    i.fa.fa-play
                  |  Contextual
                  b.text-danger  - Danger
                li.list-group-item.list-group-item-alert
                  button#c-alert.btn.btn-default.btn-sm.play.mr10
                    i.fa.fa-play
                  |  Contextual
