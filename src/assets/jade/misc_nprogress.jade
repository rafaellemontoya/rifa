                li.list-group-item.bg-light
                  h4.mv5 Page Loader Positions
                li.list-group-item
                  button#p-0.btn.btn-default.btn-sm.play.mr10
                    i.fa.fa-play
                  |  Position Top -
                  = ' '
                  b barPos:
                  |  '' (default no class required)
                li.list-group-item
                  button#p-1.btn.btn-default.btn-sm.play.mr10
                    i.fa.fa-play
                  |  Position Header -
                  = ' '
                  b barPos:
                  |  'npr-header'
                li.list-group-item
                  button#p-2.btn.btn-default.btn-sm.play.mr10
                    i.fa.fa-play
                  |  Position Bottom -
                  = ' '
                  b barPos:
                  |  'npr-bottom'
                li.list-group-item.list-group-item-danger.hidden
                  button#b-100.btn.btn-default.btn-sm.play.mr10
                    i.fa.fa-play
                  |  NProgress
